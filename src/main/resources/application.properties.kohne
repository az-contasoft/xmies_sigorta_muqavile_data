
spring.application.name=xmies_sigorta_muqavile_data

server.port=14400

spring.redis.host=redis-db-tovuz

eureka.client.serviceUrl.defaultZone=http://naming-server-tovuz:8761/eureka/
eureka.client.instance.preferIpAddress=true
spring.cache.type=none
logging.level.root=info
logging.pattern.console=%n%d{yyyy-MM-dd HH:mm:ss.SSS} --> %highlight(%class{1}.%M %L) --> : %msg%n
logging.pattern.file=%n%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} %M %L - %msg%n
#logging.path=logs2
spring.main.allow-bean-definition-overriding=true
ribbon.eager-load.enabled=true